	<StartupCode><![CDATA[
for (Caregiver c : caregivers) {
    PatientWithDem p = patientWithDem.get(c.getIndex());
    Home h = homes.get(c.getIndex());
    c.myPatient = p;
    c.myHome = h;
    p.myCaregiver = c;
    p.myHome = h;
}
traceln("Linked caregivers to patients");

// Initialize stress indicators
for (Caregiver c : caregivers) {
    if (c.caregiverStressStatechart != null) {
        c.updateDistressColour();
    } else {
        traceln("WARNING: Statechart not initialized");
    }
}
traceln("Stress indicators initialized");

// Position homes
positionHomesOnGrid();

// Position agents
positionAgentsInHomes();


traceln("===== Initialization Complete =====");]]></StartupCode>
